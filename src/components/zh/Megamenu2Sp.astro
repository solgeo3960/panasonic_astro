      <div class="accordion-header js-accordion-header">
        <h3 class="">审批、手续
        </h3>
      </div>
      <div class="accordion-content">
        <ul>
          {
            kessaiLinks.map((item) => (
              <li>
                <a href={item.urlZh}>{item.textZh}</a>
              </li>
            ))
          }
        </ul>
      </div>
    </div>
    <div class="accordion">
      <div class="accordion-header js-accordion-header">
        <h3>品牌的制定、修改和废除
